        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.4.2",
        "@mui/material": "^5.7.0",
        "@mui/styles": "^5.3.0",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.3",
